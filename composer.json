{
    "name": "appsco/rackspace-cli",
    "license": "MIT",
    "type": "project",
    "description": "Rackspace API cli PHP client",
    "autoload": {
        "psr-0": { "": "src/" }
    },
    "repositories": [
        { "type": "vcs", "url": "git@github.com:tmilos/php-opencloud.git" }
    ],
    "require": {
        "php": ">=5.4.1",
        "symfony/console": "~2.4",
        "symfony/finder": "~2.4",
        "rackspace/php-opencloud": "dev-RegionLessCatalogService"
    },
    "config": {
        "bin-dir": "bin"
    },
    "prefer-stable": true,
    "minimum-stability": "stable"
}
